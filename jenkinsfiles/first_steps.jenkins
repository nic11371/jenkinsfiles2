#!groovy
// Check ub1 host properties
properties([disableConcurrentBuilds()])

pipeline {
    agent { 
        label 'master || built-in'
        }
    options {
        buildDiscarder(logRotator(numToKeepStr: '10', artifactNumToKeepStr: '10'))
        timestamps()
    }
    stages {
        stage("First step") {
            steps {
                sh 'ssh gost@ub1 \'hostname\''
            }
        }
        stage("Second step") {
            steps {
                sh 'ssh gost@ub1 \'uptime\''
            }
        }
    }
}

